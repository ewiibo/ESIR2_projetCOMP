package org.xtext.generator;

public class TroisAdd {
	

}

@SuppressWarnings("hiding")
class Quadruplet <Op,R, O1,O2>{
	private Op operateur;
	private R resultat;
	private O1 arg1;
	private O2 arg2;
	
	public Quadruplet(Op operateur, R resultat) {
		super();
		this.operateur = operateur;
		this.resultat = resultat;
	}
	public Quadruplet(Op operateur, R resultat, O1 arg1) {
		super();
		this.operateur = operateur;
		this.resultat = resultat;
		this.arg1 = arg1;
	}
	public Quadruplet(Op operateur, R resultat, O1 arg1, O2 arg2) {
		super();
		this.operateur = operateur;
		this.resultat = resultat;
		this.arg1 = arg1;
		this.arg2 = arg2;
	}
	public Op getOperateur() {
		return operateur;
	}
	public void setOperateur(Op operateur) {
		this.operateur = operateur;
	}
	public R getResultat() {
		return resultat;
	}
	public void setResultat(R resultat) {
		this.resultat = resultat;
	}
	public O1 getArg1() {
		return arg1;
	}
	public void setArg1(O1 arg1) {
		this.arg1 = arg1;
	}
	public O2 getArg2() {
		return arg2;
	}
	public void setArg2(O2 arg2) {
		this.arg2 = arg2;
	}
	@Override
	public String toString() {
		return "Quadruplet [operateur=" + operateur + ", resultat=" + resultat + ", arg1=" + arg1 + ", arg2=" + arg2
				+ "]";
	}
	
	
	
}